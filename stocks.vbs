Sub PracticeA():'   Set an initial name for holding the ticker information.Dim ticker_name As String'   Set an initial variable for holding the total per ticker.Dim ticker_total As Doubleticker_total = 0'   Keep track of each ticker category in the summary table.Dim Summary_Table_Row As IntegerSummary_Table_Row = 2' Loop through all credit card purchases.For i = 2 To 1000        '   Set ticker name.    ticker_name = Cells(i, 1).Value        '   Check if we are still in the same ticker category, and if it is not...        If Cells(i + 1, 1).Value <> Cells(i, 1).Value Then        '   Print the ticker category in the Summary Table.    Cells(Summary_Table_Row, 10).Value = ticker_name        '   Print the ticker amount into the Summary Table.    Cells(Summary_Table_Row, 11).Value = ticker_total        '   Add 1 to the Summary Table.        Summary_Table_Row = Summary_Table_Row + 1        '   Reset the ticker total.    ticker_total = 0        '   If the cell immediately following a row is the same ticker...            Else          '   Add to the brand total.    ticker_total = ticker_total + Cells(i, 7).Value                    End If            Next i     End Sub